-- INFORME 1: Simulaci\'f3n de sueldo total con bono de jefatura\par
SELECT \par
    id_empleado AS "IDENTIFICADOR",\par
    nombres || ' ' || apellido_paterno || ' ' || apellido_materno AS "NOMBRE COMPLETO",\par
    sueldo_base AS "SALARIO",\par
