-- =============================================\par
-- CASO 4: RECUPERACI\'d3N DE DATOS (MEJORADO)\par
-- =============================================\par
\par
-- INFORME 1: Simulaci\'f3n de sueldo total con bono de jefatura\par
-- Usando IN para el campo activo\par
SELECT \par
    id_empleado AS "IDENTIFICADOR",\par
    nombres || ' ' || apellido_paterno || ' ' || apellido_materno AS "NOMBRE COMPLETO",\par
    sueldo_base AS "SALARIO",\par
    bono_jefatura AS "BONIFICACION",\par
    sueldo_base + NVL(bono_jefatura, 0) AS "SALARIO SIMULADO"\par
FROM EMPLEADO\par
WHERE activo IN ('S', 's')  -- Usando IN en lugar de =\par
    AND bono_jefatura IS NOT NULL\par
ORDER BY "SALARIO SIMULADO" DESC, apellido_paterno DESC;\par
\par
-- INFORME 2: Empleados con sueldo entre $550.000 y $800.000 con posible aumento del 8%\par
-- Ya estamos usando BETWEEN aqu\'ed\par
SELECT \par
    nombres || ' ' || apellido_paterno || ' ' || apellido_materno AS "EMPLEADO",\par
    sueldo_base AS "SUELDO",\par
    '8%' AS "POSIBLE AUMENTO",\par
    sueldo_base * 1.08 AS "SALARIO SIMULADO"\par
FROM EMPLEADO\par
WHERE sueldo_base BETWEEN 550000 AND 800000  -- Usando BETWEEN\par
ORDER BY sueldo_base ASC;\par
\par
-- CONSULTAS ADICIONALES DEMOSTRANDO EL USO DE LIKE, BETWEEN E IN:\par
\par
-- Consulta 3: Empleados cuyos nombres empiezan con "Mar" (usando LIKE)\par
SELECT \par
    id_empleado AS "ID",\par
    nombres || ' ' || apellido_paterno AS "NOMBRE",\par
    sueldo_base AS "SALARIO"\par
FROM EMPLEADO\par
WHERE nombres LIKE 'Mar%'  -- Usando LIKE con comod\'edn %\par
ORDER BY apellido_paterno;\par
\par
-- Consulta 4: Empleados de ciertas AFPs espec\'edficas (usando IN)\par
SELECT \par
    e.id_empleado AS "ID",\par
    e.nombres || ' ' || e.apellido_paterno AS "NOMBRE",\par
    a.nombre AS "AFP",\par
    e.sueldo_base AS "SALARIO"\par
FROM EMPLEADO e\par
INNER JOIN AFP a ON e.id_afp = a.id_afp\par
WHERE a.nombre IN ('Capital', 'Habitat', 'Provida')  -- Usando IN con lista de valores\par
ORDER BY a.nombre, e.sueldo_base DESC;\par
\par
-- Consulta 5: Empleados con email de dominio espec\'edfico (usando LIKE)\par
SELECT \par
    id_empleado AS "ID",\par
    nombres || ' ' || apellido_paterno AS "NOMBRE",\par
    email AS "CORREO",\par
    sueldo_base AS "SALARIO"\par
FROM EMPLEADO\par
WHERE email LIKE '%@minimarket.com'  -- Usando LIKE para dominio de email\par
ORDER BY nombres;\par
\par
-- Consulta 6: Empleados con sueldos en rangos espec\'edficos (usando BETWEEN y OR)\par
SELECT \par
    id_empleado AS "ID",\par
    nombres || ' ' || apellido_paterno AS "NOMBRE",\par
    sueldo_base AS "SALARIO",\par
    CASE \par
        WHEN sueldo_base BETWEEN 400000 AND 600000 THEN 'Rango Bajo'\par
        WHEN sueldo_base BETWEEN 600001 AND 800000 THEN 'Rango Medio' \par
        WHEN sueldo_base > 800000 THEN 'Rango Alto'\par
    END AS "CATEGORIA_SALARIAL"\par
FROM EMPLEADO\par
WHERE sueldo_base BETWEEN 400000 AND 600000  -- Rango bajo\par
   OR sueldo_base BETWEEN 700000 AND 900000  -- Rango medio-alto\par
ORDER BY sueldo_base DESC;\par
\par
-- Consulta 7: Buscar empleados por patrones en apellidos (usando LIKE con m\'faltiples condiciones)\par
SELECT \par
    id_empleado AS "ID",\par
    nombres || ' ' || apellido_paterno || ' ' || apellido_materno AS "NOMBRE_COMPLETO",\par
    sueldo_base AS "SALARIO"\par
FROM EMPLEADO\par
WHERE apellido_paterno LIKE 'G%'  -- Apellidos que empiezan con G\par
   OR apellido_materno LIKE '%ez'  -- Apellidos que terminan con "ez"\par
ORDER BY apellido_paterno;\par
\par
-- Consulta 8: Empleados con fechas de contrato en rangos espec\'edficos (usando BETWEEN con fechas)\par
SELECT \par
    id_empleado AS "ID",\par
    nombres || ' ' || apellido_paterno AS "NOMBRE",\par
    fecha_contrato AS "FECHA_CONTRATO",\par
    sueldo_base AS "SALARIO"\par
FROM EMPLEADO\par
WHERE fecha_contrato BETWEEN TO_DATE('2020-01-01', 'YYYY-MM-DD') \par
                        AND TO_DATE('2022-12-31', 'YYYY-MM-DD')  -- BETWEEN con fechas\par
ORDER BY fecha_contrato DESC;\par
Resumen de los operadores utilizados:\par
\par
BETWEEN: Para rangos de valores (sueldos, fechas)\par
\par
LIKE: Para b\'fasqueda de patrones en texto (nombres, emails, apellidos)\par
\par
'Mar%' - empieza con "Mar"\par
\par
'%ez' - termina con "ez"\par
\par
'%@minimarket.com' - dominio espec\'edfico\par
\par
IN: Para listas de valores espec\'edficos\par
\par
IN ('S', 's') - valores activos\par
\par
IN ('Capital', 'Habitat', 'Provida') - AFPs espec\'edficas\par
}
