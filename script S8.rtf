    nombres || ' ' || apellido_paterno || ' ' || apellido_materno AS "EMPLEADO",\par
    sueldo_base AS "SUELDO",\par
    '8%' AS "POSIBLE AUMENTO",\par
    sueldo_base * 1.08 AS "SALARIO SIMULADO"\par
FROM EMPLEADO\par
WHERE sueldo_base BETWEEN 550000 AND 800000\par
ORDER BY sueldo_base ASC;\par
