-- Crear tablas en orden secuencial seg\'fan dependencias\par
\par
-- Tablas fuertes (independientes)\par
CREATE TABLE AFP (\par
    id_afp NUMBER GENERATED ALWAYS AS IDENTITY (START WITH 210 INCREMENT BY 6),\par
    nombre VARCHAR2(100) NOT NULL,\par
    CONSTRAINT afp_pk PRIMARY KEY (id_afp)\par
);\par
\par
CREATE TABLE SALUD (\par
    id_salud NUMBER GENERATED ALWAYS AS IDENTITY (START WITH 2050 INCREMENT BY 10),\par
    nombre VARCHAR2(100) NOT NULL,\par
    CONSTRAINT salud_pk PRIMARY KEY (id_salud)\par
);\par
\par
CREATE TABLE REGION (\par
    id_region NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(100) NOT NULL,\par
    CONSTRAINT region_nombre_un UNIQUE (nombre)\par
);\par
\par
CREATE TABLE CIUDAD (\par
    id_ciudad NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(100) NOT NULL,\par
    id_region NUMBER NOT NULL,\par
    CONSTRAINT ciudad_region_fk FOREIGN KEY (id_region) REFERENCES REGION(id_region)\par
);\par
\par
CREATE TABLE COMUNA (\par
    id_comuna NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(100) NOT NULL,\par
    id_ciudad NUMBER NOT NULL,\par
    CONSTRAINT comuna_ciudad_fk FOREIGN KEY (id_ciudad) REFERENCES CIUDAD(id_ciudad)\par
);\par
\par
CREATE TABLE MARCA (\par
    id_marca NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(100) NOT NULL,\par
    CONSTRAINT marca_nombre_un UNIQUE (nombre)\par
);\par
\par
CREATE TABLE CATEGORIA (\par
    id_categoria NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(100) NOT NULL,\par
    descripcion VARCHAR2(500),\par
    CONSTRAINT categoria_nombre_un UNIQUE (nombre)\par
);\par
\par
CREATE TABLE PROVEEDOR (\par
    id_proveedor NUMBER PRIMARY KEY,\par
    rut VARCHAR2(12) NOT NULL,\par
    nombre VARCHAR2(200) NOT NULL,\par
    giro VARCHAR2(200),\par
    direccion VARCHAR2(300),\par
    telefono VARCHAR2(20),\par
    email VARCHAR2(150),\par
    id_comuna NUMBER NOT NULL,\par
    CONSTRAINT proveedor_rut_un UNIQUE (rut),\par
    CONSTRAINT proveedor_email_un UNIQUE (email),\par
    CONSTRAINT proveedor_comuna_fk FOREIGN KEY (id_comuna) REFERENCES COMUNA(id_comuna)\par
);\par
\par
CREATE TABLE MEDIO_PAGO (\par
    id_medio_pago NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(100) NOT NULL,\par
    descripcion VARCHAR2(300),\par
    CONSTRAINT medio_pago_nombre_un UNIQUE (nombre)\par
);\par
\par
-- Tablas dependientes\par
CREATE TABLE EMPLEADO (\par
    id_empleado NUMBER PRIMARY KEY,\par
    rut VARCHAR2(12) NOT NULL,\par
    nombres VARCHAR2(100) NOT NULL,\par
    apellido_paterno VARCHAR2(100) NOT NULL,\par
    apellido_materno VARCHAR2(100),\par
    fecha_nacimiento DATE,\par
    direccion VARCHAR2(300),\par
    telefono VARCHAR2(20),\par
    email VARCHAR2(150),\par
    fecha_contrato DATE NOT NULL,\par
    sueldo_base NUMBER(10,2) NOT NULL,\par
    bono_jefatura NUMBER(10,2),\par
    activo CHAR(1) DEFAULT 'S' NOT NULL,\par
    id_afp NUMBER NOT NULL,\par
    id_salud NUMBER NOT NULL,\par
    id_comuna NUMBER NOT NULL,\par
    jefe_id NUMBER,\par
    CONSTRAINT empleado_rut_un UNIQUE (rut),\par
    CONSTRAINT empleado_email_un UNIQUE (email),\par
    CONSTRAINT empleado_activo_ck CHECK (activo IN ('S', 'N')),\par
    CONSTRAINT empleado_afp_fk FOREIGN KEY (id_afp) REFERENCES AFP(id_afp),\par
    CONSTRAINT empleado_salud_fk FOREIGN KEY (id_salud) REFERENCES SALUD(id_salud),\par
    CONSTRAINT empleado_comuna_fk FOREIGN KEY (id_comuna) REFERENCES COMUNA(id_comuna),\par
    CONSTRAINT empleado_jefe_fk FOREIGN KEY (jefe_id) REFERENCES EMPLEADO(id_empleado)\par
);\par
\par
CREATE TABLE PRODUCTO (\par
    id_producto NUMBER PRIMARY KEY,\par
    nombre VARCHAR2(200) NOT NULL,\par
    descripcion VARCHAR2(500),\par
    precio_venta NUMBER(10,2) NOT NULL,\par
    stock_actual NUMBER(8) DEFAULT 0 NOT NULL,\par
    stock_minimo NUMBER(8) DEFAULT 3 NOT NULL,\par
    id_categoria NUMBER NOT NULL,\par
    id_marca NUMBER NOT NULL,\par
    id_proveedor NUMBER NOT NULL,\par
    CONSTRAINT producto_categoria_fk FOREIGN KEY (id_categoria) REFERENCES CATEGORIA(id_categoria),\par
    CONSTRAINT producto_marca_fk FOREIGN KEY (id_marca) REFERENCES MARCA(id_marca),\par
    CONSTRAINT producto_proveedor_fk FOREIGN KEY (id_proveedor) REFERENCES PROVEEDOR(id_proveedor)\par
);\par
\par
CREATE TABLE VENDEDOR (\par
    id_vendedor NUMBER PRIMARY KEY,\par
    comision NUMBER(5,4) DEFAULT 0 NOT NULL,\par
    id_empleado NUMBER NOT NULL,\par
    CONSTRAINT vendedor_empleado_fk FOREIGN KEY (id_empleado) REFERENCES EMPLEADO(id_empleado),\par
    CONSTRAINT vendedor_comision_ck CHECK (comision BETWEEN 0 AND 0.25)\par
);\par
\par
CREATE TABLE VENTA (\par
    id_venta NUMBER GENERATED ALWAYS AS IDENTITY (START WITH 5050 INCREMENT BY 3),\par
    fecha_venta DATE DEFAULT SYSDATE NOT NULL,\par
    total_venta NUMBER(12,2) NOT NULL,\par
    id_vendedor NUMBER NOT NULL,\par
    id_medio_pago NUMBER NOT NULL,\par
    CONSTRAINT venta_pk PRIMARY KEY (id_venta),\par
    CONSTRAINT venta_vendedor_fk FOREIGN KEY (id_vendedor) REFERENCES VENDEDOR(id_vendedor),\par
    CONSTRAINT venta_medio_pago_fk FOREIGN KEY (id_medio_pago) REFERENCES MEDIO_PAGO(id_medio_pago)\par
);\par
\par
CREATE TABLE DETALLE_VENTA (\par
    id_detalle_venta NUMBER PRIMARY KEY,\par
    cantidad NUMBER(5) NOT NULL,\par
    precio_unitario NUMBER(10,2) NOT NULL,\par
    subtotal NUMBER(12,2) NOT NULL,\par
    id_venta NUMBER NOT NULL,\par
    id_producto NUMBER NOT NULL,\par
    CONSTRAINT detalle_venta_cantidad_ck CHECK (cantidad >= 1),\par
    CONSTRAINT detalle_venta_venta_fk FOREIGN KEY (id_venta) REFERENCES VENTA(id_venta),\par
    CONSTRAINT detalle_venta_producto_fk FOREIGN KEY (id_producto) REFERENCES PRODUCTO(id_producto)\par
);\par
